<?xml version="1.0" encoding="utf-8"?>
<resources>
     <declare-styleable name="Banner">
         <attr name="position"/>
     </declare-styleable>

     <declare-styleable name="BannerItem">
         <attr name="active" format="boolean"/>
     </declare-styleable>

    <attr name="position">
        <enum name="nearby" value="0"></enum>
        <enum name="where_to_go" value="1"></enum>
        <enum name="activity" value="2"></enum>
        <enum name="profile" value="3"></enum>
    </attr>
</resources>
